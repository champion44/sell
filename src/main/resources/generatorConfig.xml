        <!-- *Mapper.xml 文件的位置 -->
        <sqlMapGenerator targetPackage="com.imooc.mapperXml" targetProject="src/main/java">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>
 
        <!-- Mapper 接口文件的位置 -->
        <javaClientGenerator targetPackage="com.imooc.mapper" targetProject="src/main/java" type="XMLMAPPER">
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>
 
        <table tableName="wx_user" domainObjectName="WxUser" enableCountByExample="false" enableDeleteByExample="false" enableSelectByExample="false" enableUpdateByExample="false"/>
    </context>
</generatorConfiguration>
